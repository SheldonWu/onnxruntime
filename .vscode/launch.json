    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: build scripts",
            "python": "${command:python.interpreterPath}",
            "type": "python",
            "request": "launch",
            "program": "/workspace/onnxruntime/tools/ci_build/build.py",
            "console": "integratedTerminal",
            //"env": {"CUDA_VISIBLE_DEVICES":"0,1"},
            "args": ["--build_dir", "/workspace/onnxruntime/build/Linux","--skip_submodule_sync",
            "--config", "RelWithDebInfo",
            "--build_shared_lib", "--parallel"],
            "justMyCode": true
        },
        {
            "name": "cxx-debug-sample",
            "type": "cppdbg",
            "request": "launch",
            //"program": "/workspace/onnxruntime/build/Linux/RelWithDebInfo/fns_candy_style_transfer",
            //"args": ["/workspace/model/candy.onnx", "/workspace/model/c002_i.png","/workspace/model/c002_o.png","cpu" ],
            "program": "/workspace/onnxruntime/build/Linux/RelWithDebInfo/mnist",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "BuildSample",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
